#include <iostream>

using namespace std;

int main() {
  int row, column;
  while ((row != 0) && (column != 0)){
    cout << "Enter number of rows and columns: \n";
    cin >> row >> column;
    for (int r = 0; r < row; r++){
      for (int c = 0; c < column; c++){
	cout << "X.";
	
      }
      cout << "\n";
    }
  }
    
  return 0;  
}
